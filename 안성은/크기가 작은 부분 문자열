class Solution {
    public int solution(String t, String p) {
        int answer = 0;
        long compareNumber = Long.parseLong(p);
        for ( int i = 0; i <= t.length() - p.length(); i++) {
            long number = Long.parseLong(t.substring(i,i + p.length()));
            if ( compareNumber - number >= 0) answer++;
        }
        return answer;
    }
}
